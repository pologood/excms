<!-- 2张图片横排 -->
<script id="img_img" type="text/html">
    <?
	var nData = data, flag = false;
	//如果最后2张是图片，图片并排排列
	if (data[data.length-1].img && data[data.length-2].img) {
		var ldata = data[data.length-1],
			rdata = data[data.length-2],
			nData = data.splice(0, data.length-2);
			flag = true;
	} else {
		nData = data;
	}

    //这里的数据已经去掉了最后2个数据（如果最后2张是图片的情况下）
	include('img_text',{data:nData});

	if (flag) {
		var ldata = data[data.length-1],
			rdata = data[data.length-2];
	?>
