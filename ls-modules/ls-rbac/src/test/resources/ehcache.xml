<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
	monitoring="autodetect" dynamicConfig="true">
	<!-- <ehcache Check="false"> -->
	<diskStore path="java.io.tmpdir" />
	<defaultCache name="defaultCache" timeToIdleSeconds="60"
		timeToLiveSeconds="60" maxElementsInMemory="200000" eternal="false"
		overflowToDisk="true" maxElementsOnDisk="10000000" diskPersistent="false"
		diskExpiryThreadIntervalSeconds="120" memoryStoreEvictionPolicy="LRU" />
	<!-- <cache name="cn.lonsun.UserDemo.entity.UserEO" timeToIdleSeconds="30" 
		timeToLiveSeconds="30" maxElementsInMemory="200000" eternal="false" overflowToDisk="true" 
		maxElementsOnDisk="10000000" diskPersistent="false" diskExpiryThreadIntervalSeconds="120" 
		memoryStoreEvictionPolicy="LRU" /> -->
</ehcache>